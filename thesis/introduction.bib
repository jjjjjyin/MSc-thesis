journal = {J. Mater. Chem. A},
number = {38},
pages = {19218--19253},
publisher = {Royal Society of Chemistry},
title = {{Poly(ethylene oxide)-based electrolytes for lithium-ion batteries}},
url = {http://dx.doi.org/10.1039/C5TA03471J},
volume = {3},
year = {2015}
}
