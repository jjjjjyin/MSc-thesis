@incollection{Buckley1975,
abstract = {Two available theories of melting in extended chain polymer crystals are critically examined and compared with calorimetric data. Melting points, T m , and enthalpy of fusion, $\Delta$H, have been measured for extended-chain crystals of poly(ethylene-oxide) fractions, covering a wide range of molecular weights. It appears that the theory of Flory and Vrij is the most appropriate to describe the present results and literature data. Application of this theory to experiments yields T m (∞) = (68.9 ± 0.4) °C and suggests that the value of the surface free energy, $\sigma$ e , lies within 0.34 and 0.81 Kcal/mole, its enthalpic part ($\Delta$H e /2) being (4.18 ± 0.22) Kcal/mole. Such a low value of $\sigma$ e may be attributed to hydrogen bonding of OH end-groups located in the disordered surface layer of closely stacked crystalline lamellae.},
author = {Buckley, C P and Kovacs, A J},
booktitle = {Polym. Aspekte},
doi = {10.1007/BFb0117122},
file = {:Users/junjie/Library/Application Support/Mendeley Desktop/Downloaded/Buckley, Kovacs - 1975 - Melting behaviour of low molecular weight poly (ethylene-oxide) fractions.pdf:pdf},
pages = {44--52},
title = {{Melting behaviour of low molecular weight poly (ethylene-oxide) fractions}},
url = {http://www.springerlink.com/index/10.1007/BFb0117122},
year = {1975}
}
@article{Brady2017,
abstract = {In many respects, polymeric materials have a preeminent position among excipients, as they form the backbone of modern pharmaceutical formulation practice and drug delivery technologies in particular. With the exception of a few technologies, the majority of bioavailability enhancement, modified-release, and controlled-release technologies rely on the unique properties of polymers to achieve drug-release control through a variety of mechanisms. Polymeric excipients are numerous and very diverse of natural, semisynthetic, or synthetic origin, depending on the source and method of preparation. They can also be classified alternatively according to their chemical composition, chain shape, comonomer sequence, and their performance. Our objective is to provide the user and formulator of polymeric excipients an operational and practical guide upon which to base rational formulation decisions and understand the molecular origins of polymer performance and structure- property relationships. The chapter delves into the fundamentals and measurement of foundational polymer compositional, structural, and property traits. Additionally the importance of structure- property relationships in modern formulation development is highlighted with numerous practical examples. The chapter concludes with a broad overview of the major classes of natural, semisynthetic and synthetic pharmaceutical polymers.},
author = {Brady, J. and D{\"{u}}rig, T. and Lee, P.I. and Li, J.-X.},
doi = {10.1016/B978-0-12-802447-8.00007-8},
isbn = {9780128024478},
journal = {Dev. Solid Oral Dos. Forms},
month = {jan},
pages = {181--223},
publisher = {Academic Press},
title = {{Polymer Properties and Characterization}},
url = {https://www.sciencedirect.com/science/article/pii/B9780128024478000078},
year = {2017}
}
