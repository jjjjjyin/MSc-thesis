keywords = {(Keywords: polymer crystallization,normal alkanes,surface poisoning)},
number = {12},
pages = {2517--2524},
title = {{Growth and nucleation rate minima in long n-alkanes}},
url = {https://ac-els-cdn-com.proxy.lib.uwaterloo.ca/0032386196853675/1-s2.0-0032386196853675-main.pdf?{\_}tid=38b8214c-0ce8-458f-836c-84f1da08ac1f{\&}acdnat=1538341305{\_}2895c7cf53d8f64f17c70bbd23a63d4a},
