keywords = {crystal growth,polymer crystallization,primary nucleation,secondary nucleation,theories},
number = {1},
pages = {4},
title = {{A Review on Polymer Crystallization Theories}},
url = {http://www.mdpi.com/2073-4352/7/1/4},
volume = {7},
year = {2016}
}
